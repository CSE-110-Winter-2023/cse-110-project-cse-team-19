<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompassActivity">


    <TextView
        android:id="@+id/locationText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="23dp"
        android:text="locationCoordinates"
        app:layout_constraintBottom_toTopOf="@+id/orientation"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/orientation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="68dp"
        android:text="orientation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/compassLayout"

        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/compass_image"


            android:layout_width="match_parent"

            android:layout_height="match_parent"
            android:layout_marginStart="40dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="40dp"
            android:src="@drawable/compass_ui"

            app:layout_constraintBottom_toBottomOf="parent"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.757"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/red_icon"
            android:layout_width="39dp"
            android:layout_height="38dp"
            app:layout_constraintBottom_toTopOf="@+id/compass_image"


            app:layout_constraintCircle="@+id/compassLayout"
            app:layout_constraintCircleAngle="210"
            app:layout_constraintCircleRadius="150dp"
            app:layout_constraintEnd_toStartOf="@+id/compass_image"
            app:layout_constraintStart_toEndOf="@+id/compass_image"
            app:layout_constraintTop_toBottomOf="@+id/compass_image"
            app:srcCompat="@drawable/icon_red" />

        <ImageView
            android:id="@+id/blue_icon"
            android:layout_width="39dp"
            android:layout_height="38dp"
            app:layout_constraintBottom_toTopOf="@+id/compass_image"

            app:layout_constraintCircle="@+id/compassLayout"
            app:layout_constraintCircleAngle="0"
            app:layout_constraintCircleRadius="150dp"
            app:layout_constraintEnd_toStartOf="@+id/compass_image"
            app:layout_constraintStart_toEndOf="@+id/compass_image"
            app:layout_constraintTop_toBottomOf="@+id/compass_image"
            app:srcCompat="@drawable/icons_blue" />

        <ImageView
            android:id="@+id/purple_icon"
            android:layout_width="39dp"
            android:layout_height="38dp"
            app:layout_constraintBottom_toTopOf="@+id/compass_image"
            app:layout_constraintCircle="@+id/compassLayout"
            app:layout_constraintCircleAngle="320"
            app:layout_constraintCircleRadius="150dp"
            app:layout_constraintEnd_toStartOf="@+id/compass_image"
            app:layout_constraintStart_toEndOf="@+id/compass_image"
            app:layout_constraintTop_toBottomOf="@+id/compass_image"
            app:srcCompat="@drawable/icons_purple" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/homeLabelDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="My Home"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/familyLabelDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Family Home"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homeLabelDisplay" />

    <TextView
        android:id="@+id/friendLabelDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Friend Home"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/familyLabelDisplay" />

    <Button
        android:id="@+id/button"
        android:layout_width="169dp"
        android:layout_height="66dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:onClick="backToCoordinates"
        android:text="Change House Coordinates"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toEndOf="@+id/homeLabelDisplay"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/red" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="51dp"
        app:layout_constraintStart_toEndOf="@+id/familyLabelDisplay"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/blue" />

</androidx.constraintlayout.widget.ConstraintLayout>


